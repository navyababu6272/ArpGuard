

import re

def checkMAC(x):
      if re.match("[0-9a-f]{2}([-:])[0-9a-f]{2}(\\1[0-9a-f]{2}){4}$", x.lower()):
            return 1
    else:
            return 0

print checkMAC("AA:BB:CC:DD:EE:FF")
print checkMAC("00-11-22-33-44-66")
print checkMAC("1 2 3 4 5 6 7 8 9 a b c")
print checkMAC("This is not a mac")

